{
    "name": "esolangs",
    "version": "0.0.1",
    "publisher": "HactarCE",
    "engines": {
        "vscode": "^1.28.0"
    },
    "license": "WTFPL",
    "displayName": "Esoteric Programming Language Support",
    "description": "Syntax highlighting for esoteric programming languages",
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "esolang",
        "esolangs",
        "esoteric",
        "naz",
        "st@ic",
        "static",
        "stic"
    ],
    "contributes": {
        "languages": [
            {
                "id": "naz",
                "aliases": [
                    "naz"
                ],
                "extensions": [
                    ".naz"
                ],
                "configuration": "./configurations/naz.json"
            },
            {
                "id": "static",
                "aliases": [
                    "St@ic"
                ],
                "extensions": [
                    ".st@ic",
                    ".static",
                    ".stic"
                ],
                "configuration": "./configurations/static.json"
            }
        ],
        "grammars": [
            {
                "language": "naz",
                "scopeName": "source.naz",
                "path": "./syntaxes/naz.json"
            },
            {
                "language": "static",
                "scopeName": "source.static",
                "path": "./syntaxes/static.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/HactarCE/vscode-esolangs.git"
    },
    "icon": "images/icon.png"
}
